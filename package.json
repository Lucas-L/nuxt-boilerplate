{
	"name": "staky-nuxt-firebase-starter",
	"version": "0.3.0",
	"description": "Nuxt Firebase Starter by Staky",
	"author": "Staky",
	"private": true,
	"scripts": {
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"analyze": "nuxt build --analyze",
		"commit": "git add . && commit",
		"push": "git push origin",
		"pull": "git pull --rebase",
		"bump-first": "standard-version --first-release",
		"bump-minor": "standard-version --release-as minor",
		"bump-major": "standard-version --release-as major",
		"bump-patch": "standard-version --release-as patch",
		"release": "git push --follow-tags origin",
		"lint": "eslint --ext .js,.vue --ignore-path .gitignore --ignore-pattern src/modules/*/*.plugin.js ."
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint",
			"commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
		}
	},
	"dependencies": {
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
		"@nuxtjs/device": "^2.1.0",
		"@nuxtjs/firebase": "^7.5.0",
		"autoprefixer": "^10.2.5",
		"axios": "^0.21.1",
		"consola": "^2.15.3",
		"cross-env": "^5.2.0",
		"dotenv": "^8.2.0",
		"element-ui": "^2.13.2",
		"firebase": "^8.3.3",
		"gsap": "^3.6.1",
		"lodash": "^4.17.19",
		"nuxt": "^2.15.4",
		"postcss": "^8.2.10",
		"redirect-ssl": "^2.0.1",
		"stylus": "^0.54.8",
		"stylus-loader": "^5.0.0",
		"v-click-outside": "^3.1.2",
		"vue": "^2.6.12",
		"vue-fragment": "^1.5.2",
		"vue-loading-skeleton": "^1.1.9",
		"vue-mq": "^1.0.1",
		"vue-scrollto": "^2.20.0",
		"vue-timeago": "^5.1.3",
		"vuex": "^3.6.2",
		"vuex-persist": "^2.2.0"
	},
	"devDependencies": {
		"@babel/runtime-corejs3": "^7.10.4",
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-conventional": "^8.3.4",
		"@commitlint/prompt-cli": "^8.3.5",
		"@nuxt/babel-preset-app": "^2.13.3",
		"@nuxtjs/color-mode": "^2.0.5",
		"@nuxtjs/eslint-config": "^2.0.0",
		"@nuxtjs/eslint-module": "^1.0.0",
		"@nuxtjs/robots": "^2.5.0",
		"@nuxtjs/sitemap": "^2.4.0",
		"@nuxtjs/style-resources": "^1.0.0",
		"@nuxtjs/stylelint-module": "^3.1.0",
		"@nuxtjs/tailwindcss": "^4.0.3",
		"@teamnovu/nuxt-breaky": "^1.2.1",
		"@vue/cli-plugin-babel": "^4.4.6",
		"@vue/cli-plugin-eslint": "^4.4.6",
		"@vue/cli-service": "^4.4.6",
		"@vue/eslint-config-airbnb": "^5.1.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^10.1.0",
		"babel-plugin-component": "^1.1.1",
		"babel-preset-vue": "^2.0.2",
		"core-js": "^3.6.5",
		"eslint": "^7.4.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-webpack": "^0.12.2",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-nuxt": "^1.0.0",
		"eslint-plugin-vue": "^6.2.2",
		"husky": "^4.2.5",
		"nodemon": "^1.11.0",
		"path": "^0.12.7",
		"prettier": "^2.0.5",
		"standard-version": "^9.2.0",
		"tailwindcss": "^2.1.1",
		"tailwindcss-dark-mode": "^1.1.7"
	}
}
